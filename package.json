{
  "name": "leadscout-ai-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "LeadScout AI - Facebook Lead Generation Platform",
  "workspaces": [
    "extension",
    "backend"
  ],
  "scripts": {
    "dev": "npm run dev:backend & npm run dev:extension",
    "dev:extension": "npm run dev --workspace=extension",
    "dev:backend": "npm run dev --workspace=backend",
    "build": "npm run build --workspaces",
    "build:extension": "npm run build --workspace=extension",
    "build:backend": "npm run build --workspace=backend",
    "lint": "npm run lint --workspaces --if-present",
    "test": "npm run test --workspaces --if-present",
    "clean": "rm -rf node_modules extension/node_modules backend/node_modules"
  },
  "keywords": [
    "facebook",
    "lead-generation",
    "chrome-extension",
    "ai"
  ],
  "author": "",
  "license": "PRIVATE"
}
